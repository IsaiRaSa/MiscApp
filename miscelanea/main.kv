<SignoutPopup>:
    size_hint: .25, .25
    title: 'Signout'
    BoxLayout:
        orientation: 'vertical'
        Label:
            text: '¿Desea salirse de su cuenta?'
        BoxLayout:
            Button:
                text: 'Sí'
                on_release: root.accept()
            Button:
                text: 'No'
                on_release: root.dismiss()


<MainWindow>:
    id: main_win

    ScreenManager:
        id: scrn_mngr_main

        Screen:
            id: scrn_si
            name: 'scrn_si'
        Screen:
            id: scrn_admin
            name: 'scrn_admin'
        Screen:
            id: scrn_home
            name: 'scrn_home'